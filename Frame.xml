				</Scripts>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
				this:RegisterForClicks("LeftButtonUp","RightButtonUp")
				this:RegisterForDrag("LeftButton","RightButton")
			</OnLoad>
			<OnDragStart>
				this:LockHighlight()
				MinimapButton_DraggingFrame:Show()
			</OnDragStart>
			<OnDragStop>
				this:UnlockHighlight()
				MinimapButton_DraggingFrame:Hide()
			</OnDragStop>
			<OnClick>
				MinimapButton_OnClick()
			</OnClick>
		</Scripts>
